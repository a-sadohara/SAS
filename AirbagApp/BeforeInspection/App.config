<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

    <appSettings>
        <!-- DB名 -->
        <add key="DBName" value="postgres"/>
        <!-- DB接続ユーザ -->
        <add key="DBUser" value="postgres"/>
        <!-- DB接続ユーザパスワード -->
        <add key="DBUserPassword" value="password"/>
        <!-- DBサーバ名 -->
        <add key="DBServerName" value="192.168.5.17"/>
        <!-- DBポート -->
        <add key="DBPort" value="5432"/>
        <!-- 号機 -->
        <add key="UnitNum" value="N4"/>
        <!-- 撮像装置部連携ディレクトリ -->
        <add key="ImagingDeviceCooperationDirectory" value="C:\撮像装置部連携ディレクトリ"/>
        <!-- リトライ試行回数 -->
        <add key="RetryTimes" value="3" />
        <!-- 連携基盤部接続先(IPアドレス) -->
        <add key="ConnectionPoint" value="192.168.2.17" />
        <!-- 連携基盤部接続パスワード -->
        <add key="ConnectionPassword" value="p@ssw0rdnec12345" />
        <!-- 連携基盤部接続ユーザー -->
        <add key="ConnectionUser" value=".\sas_admin" />
        <!-- 反物情報.ステータス_抽出除外条件(カンマ区切り) -->
        <add key="FabricInfoExceptExtractionStatus" value="0,12,99" />
        <!-- 起動対象タスク名(カンマ区切り) -->
        <add key="TaskName" value="function_301 startup,function_302 startup.,function_304 startup.,function_306 startup.,function_307 startup.,function_310 startup.,function_311 startup." />
        <!-- チェック対象実行ファイル名(カンマ区切り) -->
        <add key="ExecutionFileName" value="function_301.exe,function_302.exe,function_304.exe,function_306.exe,function_307.exe,function_310.exe,function_311.exe" />
    </appSettings>
</configuration>
